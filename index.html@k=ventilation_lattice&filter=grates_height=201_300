<script type="text/javascript">

function GetCssStyle(e){ 
    if (e.currentStyle) return e.currentStyle; 
    else if (window.getComputedStyle) return window.getComputedStyle(e,null) 
} 

/*
var elem = document.getElementById('elem');
var height_old = elem.offsetHeight;
elem.innerHTML="text";
var height = elem.offsetHeight;
alert(height_old+' '+height);
*/
var x = 0;
function resize_body_f() {
	//return;
	var txt = x += 1;
   // document.getElementById("blok1").innerHTML = txt;
	var elem2 = document.getElementById("blok_main_2");
	
	var str = GetCssStyle(elem2).width;
	var width2 = str.substr(0, str.length - 2);
	var elem21 = document.getElementById("blok_main_2_1");
	var elem22 = document.getElementById("blok_main_2_2");

	var elem22 = document.getElementById("blok_main_2_2");

	var str21 = GetCssStyle(elem21).width;
	var width21 = str21.substr(0, str21.length - 2);

	var elem2_1_x = document.getElementById("blok_main_2_1_x");
	if (elem2_1_x == null) {return;}
	var str2_1_x = GetCssStyle(elem2_1_x).width;
	var width2_1_x = str2_1_x.substr(0, str2_1_x.length - 2);

	var full_21x_count = (Number(width21))/(Number(width2_1_x)+10);  
	var info = document.getElementById("info");
	//var info1 = document.getElementById("info1");
//	info.innerHTML = "21x: "+width2_1_x + ", count: "+full_21x_count+ ", 21: "+width21;
	
	//
//	elem22.innerHTML = width;
	if (Number(width2) < Number('950')){//перенос блока инфо вниз
	//	console.log(width);
		elem21.removeAttribute("style");
		elem22.removeAttribute("style");
		elem21.className="blok_main_2_1_when_bl_main_2_2_in_new_line";
		elem22.className="blok_main_2_2_new_line";
		}else{
		elem21.className="blok_main_2_1";
		elem22.className="blok_main_2_2";
		
		//width2
		var w_left_max = 0.9* Number(width2);
		var left_num_max = Math.floor(Number(w_left_max)/(Number(width2_1_x)+10));
		var w_left_new = left_num_max * (Number(width2_1_x)+10);
		var w_left_new_procent = Math.floor(w_left_new*100/Number(width2));
		w_left_new_procent = w_left_new_procent + 1;
		var w_right_new_procent = 100-w_left_new_procent;
//		info1.innerHTML =width2_1_x;
		
		//	info1.innerHTML =w_left_new_procent + " - "+w_left_new+",  "+ w_right_new_procent;
		elem21.style.width = w_left_new_procent+"%";
		elem22.style.width = w_right_new_procent+"%";
		
		
		
		//	var x_proc = (Math.floor(Number(full_21x_count))*170*90)/Number(width21);
	//	info1.innerHTML =x_proc; 
	//	elem21.style.width = Math.floor(Number(x_proc))+"%";
	//	elem22.style.width = Math.floor(100-Math.floor(Number(x_proc)))+"%";
	//	console.log(Math.floor(Number(x_proc))+"%");
	//	console.log(Math.floor(100-Math.floor(Number(x_proc)))+"%");
		
		}
	
}

</script>


