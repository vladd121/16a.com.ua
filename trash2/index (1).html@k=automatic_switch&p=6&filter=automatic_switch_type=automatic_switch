$( "#dialog_form" ).dialog({ buttons: [ { id: "dialogSendbut", text: "Надіслати замовлення", click: function() {
send_basket_dialog({data:{bsend:"bsend",
 user_name:document.getElementById("user_name").value,
 user_phone:document.getElementById("user_phone").value,
